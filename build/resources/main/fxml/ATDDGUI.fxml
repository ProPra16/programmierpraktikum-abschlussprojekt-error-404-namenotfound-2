<HBox alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ATDDController">
   <children>
      <TabPane>
         <tabs>
            <Tab closable="false" text="ATDD">
               <content>
                  <VBox alignment="TOP_CENTER">
                     <children>
                        <Label text="ATDD Test" VBox.vgrow="ALWAYS">
                           <font>
                              <Font size="20.0" />
                           </font>
                        </Label>
                        <TextArea fx:id="ATDDtestcodefield" minHeight="300.0" VBox.vgrow="ALWAYS">
                           <VBox.margin>
                              <Insets left="10.0" right="10.0" />
                           </VBox.margin>
                        </TextArea>
                        <Label text="Terminal" VBox.vgrow="ALWAYS">
                           <font>
                              <Font size="20.0" />
                           </font>
                        </Label>
                        <TextArea fx:id="terminalfield" editable="false" minHeight="200.0" VBox.vgrow="ALWAYS">
                           <VBox.margin>
                              <Insets left="10.0" right="10.0" />
                           </VBox.margin>
                        </TextArea>
                        <Button fx:id="atddcheckbutton" mnemonicParsing="false" onAction="#check" text="Check" textFill="#44c348" VBox.vgrow="ALWAYS">
                           <font>
                              <Font name="Arial Bold" size="22.0" />
                           </font>
                           <padding>
                              <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                           </padding>
                           <VBox.margin>
                              <Insets bottom="10.0" top="10.0" />
                           </VBox.margin>
                        </Button>
                     </children>
                  </VBox>
               </content>
            </Tab>
            <Tab closable="false" text="Written Code">
               <content>
                  <VBox>
                     <children>
                        <StackPane>
                           <children>
                              <Label text="Wirtten Code">
                                 <font>
                                    <Font size="25.0" />
                                 </font>
                              </Label>
                           </children>
                        </StackPane>
                        <TextArea fx:id="writtencodearea" editable="false" prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
                           <VBox.margin>
                              <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                           </VBox.margin>
                        </TextArea>
                     </children>
                  </VBox>
               </content>
            </Tab>
         </tabs>
      </TabPane>
   </children>
</HBox>
